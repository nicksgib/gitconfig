[user]
	email = you@example.com
	name = Your Name
[alias]
       	br=branch --list -v
        bra=branch --list -av
        brd=branch -D
        brdm=branch -D master
        ci=commit
        cia=commit -a
        cl=clean -df
        co=checkout
        cot=checkout --track
        dcw=diff --color-words
        dst=diff --cached
        fe=fetch --all
        logs=log --oneline --graph
        meo=master origin/master
        pl=pull
        pom=pull origin master
        pr=remote prune origin
        ps=push origin --all
        pst=push --tags origin
        re=remote -v
        st=status -uno
        stashpatch=stash push --patch
        sub=submodule
        quickci=commit --amend --no-edit
        quickfix=commit --fixup HEAD
        quickrb=rebase -i --autosquash HEAD~2
